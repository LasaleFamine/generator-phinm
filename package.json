{
  "name": "generator-phinm",
  "version": "1.1.0",
  "description": "Scaffold out a node module",
  "license": "MIT",
  "repository": "LasaleFamine/generator-phinm",
  "author": {
    "name": "Alessio Occhipinti",
    "email": "info@godev.space",
    "url": "https://godev.space"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "release": "git pull && standard-version",
    "test": "xo && nyc ava",
    "test:travis": "yarn test && nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "files": [
    "app"
  ],
  "keywords": [
    "generator-phinm",
    "yeoman-generator",
    "plugin",
    "boilerplate",
    "template",
    "scaffold",
    "node",
    "module",
    "node_module",
    "node-module",
    "init",
    "lasalefamine",
    "phi"
  ],
  "dependencies": {
    "humanize-url": "1.0.1",
    "is-scoped": "1.0.0",
    "normalize-url": "3.0.0",
    "superb": "2.0.0",
    "underscore.string": "3.3.4",
    "yeoman-generator": "3.0.0"
  },
  "devDependencies": {
    "ava": "*",
    "codecov": "3.0.1",
    "nyc": "11.8.0",
    "pify": "3.0.0",
    "standard-version": "4.4.0",
    "xo": "*",
    "yeoman-assert": "3.1.1",
    "yeoman-test": "1.7.2"
  },
  "ava": {
    "failWithoutAssertions": false
  },
  "xo": {
    "ignores": [
      "app/templates/**"
    ]
  }
}
